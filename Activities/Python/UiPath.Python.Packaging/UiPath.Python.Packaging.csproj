    <None Include="$(OutputPath)\net5.0-windows\UiPath.Python.Host*.*json" Pack="true" PackagePath="bin\net5.0-windows7.0" />
    <None Include="$(OutputPath)\net5.0\*.*dll" Pack="true" PackagePath="bin\net5.0" />
    <None Include="$(OutputPath)\net5.0-windows\*.*dll" Pack="true" PackagePath="bin\net5.0-windows7.0" />

